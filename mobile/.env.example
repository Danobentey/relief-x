# Environment Variables (Example)
# Duplicate this file as .env (never commit real secrets) and fill in values.

APP_ENV=development

# Firebase (from google-services.json / GoogleService-Info.plist)
FIREBASE_API_KEY=YOUR_FIREBASE_API_KEY
FIREBASE_PROJECT_ID=relif-x
FIREBASE_APP_ID=YOUR_FIREBASE_APP_ID
FIREBASE_MESSAGING_SENDER_ID=1034236859926
FIREBASE_IOS_CLIENT_ID=YOUR_IOS_CLIENT_ID
FIREBASE_ANDROID_CLIENT_ID=YOUR_ANDROID_CLIENT_ID

# Google OAuth (separate from Firebase Web API key)
GOOGLE_OAUTH_CLIENT_ID=YOUR_GOOGLE_OAUTH_CLIENT_ID

# Sentry
SENTRY_DSN=YOUR_SENTRY_DSN

# RevenueCat
REVENUECAT_API_KEY_PUBLIC=YOUR_PUBLIC_REVENUCECAT_KEY
REVENUECAT_ENTITLEMENT_ID=premium

# Feature Flags (JSON string)
FEATURE_FLAGS_JSON={}
